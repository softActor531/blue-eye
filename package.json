{
  "name": "wite",
  "version": "1.2.7",
  "productName": "Wite",
  "description": "",
  "main": "app.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "cross-env NODE_NO_WARNINGS=1 electron .",
    "build": "electron-builder",
    "build:mac": "electron-builder --mac",
    "build:win": "electron-builder --win"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@emnapi/runtime": "^1.4.3",
    "auto-launch": "^5.0.6",
    "axios": "^1.10.0",
    "canvas": "^3.1.0",
    "dotenv": "^16.5.0",
    "electron-is-dev": "^3.0.1",
    "electron-store": "^10.1.0",
    "elevated": "^1.2.1",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.3",
    "pidusage": "^4.0.1",
    "ps-list": "^8.1.1",
    "sharp": "^0.34.2",
    "sudo-prompt": "^9.2.1",
    "systeminformation": "^5.27.7"
  },
  "build": {
    "appId": "com.wite.client",
    "productName": "Wite",
    "executableName": "Wite",
    "files": [
      "dist/**/*",
      "app.js",
      "package.json",
      "config.json",
      "preload.js",
      "recording.js",
      "renderer.js",
      "installers/footprint.node",
      "utils/driverInstaller.js",
      "scripts/*.*",
      "assets/*.png",
      "index.html"
    ],
    "extraResources": [
      {
        "from": "installers",
        "to": "installers"
      },
      {
        "from": "assets",
        "to": "assets"
      }
    ],
    "mac": {
      "target": "dmg",
      "icon": "assets/icon-blue.png",
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": [
        "portable",
        "dir"
      ],
      "icon": "assets/icon-blue.png"
    }
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "cross-env": "^7.0.3",
    "electron": "^36.5.0",
    "electron-builder": "^26.0.12"
  }
}
