{
  "name": "sinzo-client",
  "version": "1.2.1",
  "productName": "Sinzo Client",
  "description": "",
  "main": "app.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "cross-env NODE_NO_WARNINGS=1 electron .",
    "build": "electron-builder",
    "build:mac": "electron-builder --mac",
    "build:win": "electron-builder --win"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@emnapi/runtime": "^1.4.3",
    "auto-launch": "^5.0.6",
    "axios": "^1.10.0",
    "canvas": "^3.1.0",
    "dotenv": "^16.5.0",
    "electron-is-dev": "^3.0.1",
    "electron-store": "^10.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.3",
    "jpeg-js": "^0.4.4",
    "mic": "^2.1.2",
    "ps-list": "^8.1.1",
    "readline": "^1.3.0",
    "sharp": "^0.34.2",
    "sudo-prompt": "^9.2.1",
    "systeminformation": "^5.27.7"
  },
  "build": {
    "appId": "com.sinzo.client",
    "productName": "Sinzo Client",
    "executableName": "Sinzo-Client",
    "files": [
      "**/*"
    ],
    "extraResources": [
      {
        "from": "installers",
        "to": "installers"
      }
    ],
    "mac": {
      "target": "dir"
    },
    "win": {
      "target": [
        "portable",
        "dir"
      ]
    }
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "cross-env": "^7.0.3",
    "electron": "^36.5.0",
    "electron-builder": "^26.0.12"
  }
}
